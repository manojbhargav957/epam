import java.io.*;
import java.util.*;
public class Arrdemo3{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int temp;
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
int key=sc.nextInt();
int count=0;
for(int i=0;i<n;i++)
{
    if(a[i]==key)
    {
       count++;
       temp=i;
    }
}
if(count>0)
{
    System.out.println(temp);
}
else
{
    System.out.println("-1");
}
}
}
