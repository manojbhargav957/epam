import java.io.*;
import java.util.*;
public class Arrdemo4{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int temp;
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
for(int i=0;i<n;i++)
{
    char s=(char)a[i];
    System.out.print(s+" ");
}
}
}
